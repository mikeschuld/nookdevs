<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="600.0px"
  android:layout_height="260.0px"
  android:background="#C8C8C8"
  >
  <TextView android:layout_width="fill_parent"
  	android:layout_height="40.0px"
  	android:paddingLeft="15.0px"
  	android:id="@+id/libtext01"
	android:textStyle="bold"
	android:textSize="20px"
	android:paddingTop="10.0px"
 	/>
  <TextView android:layout_width="fill_parent"
  	android:layout_height="40.0px"
  	android:paddingLeft="15.0px"
  	android:id="@+id/libtext02"
	android:textStyle="bold"
	android:textSize="20px"
	android:paddingTop="10.0px"
	android:layout_below="@id/libtext01"
	android:text="@string/userid"
  	/>
 <EditText  android:layout_width="fill_parent"
 	android:paddingTop="10.0px"
 	android:paddingLeft="15.0px"
 	android:layout_height="50.0px"
  	android:paddingBottom="10.0px"
	android:id="@+id/libedittext02"
	android:text=""
	android:singleLine="true"
	android:layout_below="@id/libtext02"
	android:shadowColor="#000000"
	android:inputType="textWebEditText"
	android:imeOptions="actionDone|flagNoEnterAction"
  /> 
  <TextView android:layout_width="fill_parent"
  	android:layout_height="40.0px"
  	android:paddingLeft="15.0px"
  	android:id="@+id/libtext03"
	android:textStyle="bold"
	android:textSize="20px"
	android:paddingTop="10.0px"
	android:layout_below="@id/libedittext02"
	android:text="@string/password"
  	/>
 <EditText  android:layout_width="fill_parent"
 	android:paddingTop="10.0px"
 	android:paddingLeft="15.0px"
 	android:layout_height="50.0px"
  	android:paddingBottom="10.0px"
	android:id="@+id/libedittext03"
	android:text=""
	android:singleLine="true"
	android:layout_below="@id/libtext03"
	android:shadowColor="#000000"
	android:inputType="textPassword"
	android:imeOptions="actionDone|flagNoEnterAction"
  /> 
</RelativeLayout>
